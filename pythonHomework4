def minsToHours():
  number_of_minutes = 75
  hours = int(number_of_minutes/60)
  minutes = number_of_minutes%60
  print(number_of_minutes, "is ", hours, "hour and ", minutes, "minutes")

minsToHours()

def namePrinter():
  name = input("What is your name?")
  print("Hello ", name, "and welcome to CS online!")

namePrinter()

def change():
  quarterAmount = 25*int(input("How many quarters do you have?"))
  dimeAmount = 10*int(input("How many dimes do you have?"))
  nickelAmount = 5*int(input("How many nickels do you have?"))
  pennyAmount = int(input("How many pennies do you have?"))
  total = (quarterAmount + dimeAmount + nickelAmount + pennyAmount)/100
  print(total)

change()

def fToC():
  celsius = int(input("How hot is it?"))
  fahrenheit = celsius*(9/5)+32
  print(f'{fahrenheit:.1f}')

fToC()

def number_square():
  x = int(input("Input a number : "))
  x = x*2
  print(x)

number_square()
